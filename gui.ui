/* /////////////////////////////////////////////////////////////////////////////////////////////////
QPushButton */
/*Стандартное состояние для кнопки*/
QPushButton {
	border: none;		/* задает границу элемента */
} 

/* срабатывает, когда пользователь наводит на элемент мышью */
QPushButton:hover {
	border: none;												/* без границ */

	border-right:1px solid rgb(160, 160, 160);	/* С правой красной раницей */
	border-bottom:1px solid rgb(160, 160, 160);

	border-left:1px solid rgb(255, 255, 255);	/* С правой красной раницей */
	border-top:1px solid rgb(255, 255, 255);
}

/* срабатывает, при нажатии*/
QPushButton:pressed      {
	border: none;												/* без границ */

	border-left:1px solid rgb(160, 160, 160);	/* С правой красной раницей */
	border-top:1px solid rgb(160, 160, 160);

	border-right:1px solid rgb(255, 255, 255);	/* С правой красной раницей */
	border-bottom:1px solid rgb(255, 255, 255);
}
/* Состояние - не выбран*/
QPushButton {
	/* Выбор картинки*/
	image: url(:/expand2/resource/expand_2/up_black_24dp.svg);
	image: url(:/expand/resource/expand/up_black_36dp.svg);

}


/* Состояние -  выбран*/
QPushButton:checked {
	/* Выбор картинки*/
	image: url(:/expand2/resource/expand_2/down_black_36dp.svg);
	image: url(:/expand/resource/expand/down_black_36dp.svg);

}
